{
    "name": "codeblog.pro/geo-coder",
    "description": "The geo-coder package helps to determine the coordinates by an address or to get the address by the coordinates. Supports multiple providers.",
    "keywords": [
        "api", "wrapper", "geo", "geocoder", "geocoding", "yandex", "google", "coordinates", "geographical-coordinates"
    ],
    "homepage": "https://codeblog.pro",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Panov Alexey",
            "email": "panov@codeblog.pro",
            "homepage": "https://codeblog.pro/",
            "role": "Owner"
        }
    ],
    "require": {
        "php": ">=7.4",
        "codeblog.pro/geo-coordinates": "^1.0.0",
        "codeblog.pro/geo-location-address": "^0.0.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0",
        "phpstan/phpstan": "^0.12.36",
        "nunomaduro/phpinsights": "^1.14",
        "phpunit/php-code-coverage": "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CodeblogPro\\GeoCoder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeblogPro\\GeoCoder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    }
}
